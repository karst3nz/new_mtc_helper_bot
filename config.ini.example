[config]
# Токен вашего Telegram бота (получите у @BotFather)
bot_token = YOUR_BOT_TOKEN_HERE

# ID администратора бота (ваш Telegram ID)
admin_id = YOUR_ADMIN_ID_HERE

# Настройка отправки расписания (1/0)
send_rasp = 1

# Чат для бэкапа БД
backup_chat_id = 

# API_ID и API_HASH для кастомного BOT API сервера (получить можно в https://my.telegram.org/auth)
api_id = 
api_hash = 

# URL кастомного Bot API сервера (опционально, если не указан - используется официальный API)
# Пример: custom_api_url = http://127.0.0.1:8081
custom_api_url = 